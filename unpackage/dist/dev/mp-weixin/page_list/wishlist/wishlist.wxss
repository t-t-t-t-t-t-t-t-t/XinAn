.addConutBox {
  position: relative;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  top: 180rpx;
  margin-top: 20rpx;
  height: 60rpx;
  width: 80%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.addConutBox > view {
  color: #fff;
  background-color: #343434;
  border: 4rpx solid #343434;
  box-sizing: border-box;
  padding: 0px;
  height: 60rpx;
  width: 45%;
  line-heihgt: 60rpx;
  border-radius: 14rpx;
  text-align: center;
}
.addConutBox .addConut {
  position: relative;
}
.addConutBox .editCound {
  color: #000;
  background-color: #fff;
  border: 4rpx solid #343434;
}
.wishBody {
  position: relative;
  top: 240rpx;
  margin-top: 20rpx;
  width: 80%;
  padding: 0rpx 40rpx;
  margin: 0rpx auto;
  box-sizing: border-box;
  background-color: #f5f5f5;
  border-radius: 20rpx;
  border: 5rpx solid #000;
}
.wishBody .bottom {
  height: 30vh;
}
.wishBody .itemWrap {
  margin-bottom: 20rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.wishBody .itemWrap text {
  margin-left: 20rpx;
}

