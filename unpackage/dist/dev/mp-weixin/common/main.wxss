.wishBody checkbox {
  width: 40rpx;
  height: 40rpx;
}
.wishBody checkbox .wx-checkbox-input {
  border-radius: 50% !important;
  box-shadow: 0rpx 7rpx 5rpx 3rpx #d1d1d1 !important;
}
.wishBody checkbox .wx-checkbox-input-checked {
  border-radius: 50% !important;
  box-shadow: 0rpx 7rpx 5rpx 3rpx #d1d1d1 !important;
  background-color: #000;
  border: #000 solid 2rpx;
}
.wishBody checkbox .wx-checkbox-input-checked.wx-checkbox-input::before {
  color: #fff !important;
  font-weight: 900;
}
